   "source": [
    "## 给出3个整数a,b,c(0<a,b,c<=100 )，要求把3个数从小到大排序。"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      " 4 8 1\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 4 8 "
     ]
    }
   ],
   "source": [
    "def paixu(a, b, c):\n",
    "    nums_list = [a, b, c]\n",
    "    nums_list.sort()\n",
    "    for i in nums_list:\n",
    "        print(i, end=' ')\n",
    "\n",
    "a, b, c = map(int, input().split())   \n",
    "paixu(a, b, c)\n",
    "       "
   ]
