   "source": [
    "from decimal import Decimal, getcontext\n",
    "getcontext().prec = 6\n",
    "m, h = map(float, input().split())\n",
    "bmi = Decimal(m) / Decimal(h**2)\n",
    "if bmi < 18.5:\n",
    "    print('Underweight')\n",
    "elif bmi < 24:\n",
    "    print('Normal')\n",
    "else:\n",
    "    print(bmi)\n",
    "    print('Overweight')       "
   ]
  }
