    "from time import time, localtime, sleep\n",
    "print(localtime())\n",
    "print(localtime(time()))\n",
    "while True:\n",
    "    ctime = localtime()\n",
    "    sleep(1)\n",
    "    print(\"%d年%d月%d日\\t%02d:%02d:%02d\" % (ctime.tm_year, ctime.tm_mon, ctime.tm_mday, ctime.tm_hour, ctime.tm_min, ctime.tm_sec))"
